```elixir

cols = ~w(open high low close adj_close volume)
n_bitcoin_usd =
 	    bitcoin_usd |> DF.rename([:date, :open, :high, :low, :close, :adj_close, :volume])
  |> DF.mutate(
 	    for col <- across(^cols) do
 	      {col.name, (col - mean(col)) / standard_deviation(col)}
 	    end
 	  )
